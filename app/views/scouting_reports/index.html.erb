<p id="notice"><%= notice %></p>

<h1>Scouting Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scouting_reports.each do |scouting_report| %>
      <tr>
        <td><%= link_to 'Show', scouting_report %></td>
        <td><%= link_to 'Edit', edit_scouting_report_path(scouting_report) %></td>
        <td><%= link_to 'Destroy', scouting_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scouting Report', new_scouting_report_path %>
